<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.parentComp.parentcomp.controller.parentComp" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					    <IconTabBar selectedKey="__filter0" id="bar0">
					        <items>
					            <IconTabFilter text="Orders" count="22" showAll="true" id="filter0">
					                <content>
					                    <core:ComponentContainer width="100%" id="myChildContainer"/>
					                </content>
					            </IconTabFilter>
					            <IconTabFilter text="Open" count="10" icon="sap-icon://task" iconColor="Critical" id="filter1"/>
					            <IconTabFilter text="Shipped" count="5" icon="sap-icon://shipping-status" iconColor="Positive" id="filter2"/></items>
					    </IconTabBar>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>